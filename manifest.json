    {
      "name": "BonBon_Beta",
      "version": "2.0",
      "description": "Build an Extension!",
      "permissions": ["declarativeContent", 
        "storage","commands",
        "tabs",
        "alarms",
        "<all_urls>",
        "notifications"],
      "background": {
        "scripts": ["js/background.js"],
        "persistent" : false
      },
      "browser_action": {
        "default_popup": "html/popup.html",
        "default_icon": {
          "16": "images/16.png",
          "32": "images/32.png",
          "48": "images/48.png",
          "128": "images/128.png"
        }
      },
      "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
      },
      "commands": {
      "toggle-feature-foo": {
        "suggested_key": {
          "default": "Ctrl+Shift+Y",
          "mac": "Command+Shift+K"
        },
        "description": "Toggle feature foo"
      },
      "_execute_browser_action": {
        "suggested_key": {
          "windows": "Ctrl+Shift+Y",
          "mac": "Command+Shift+Y",
          "chromeos": "Ctrl+Shift+U",
          "linux": "Ctrl+Shift+J"
        }
      }
    },
      "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
      "manifest_version": 2
    }